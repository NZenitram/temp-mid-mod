<%= link_to "Create User", new_user_path %>
<%= link_to "Login", login_path %>
<%= button_to "Logout", logout_path %>

<%= form_for @link do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :url %>
  <%= f.text_field :url %>

  <%= f.submit %>
<% end %>

<% @links.each do |link| %>
  <%= link.title %>
  <%= link.url %>
<% end %>
